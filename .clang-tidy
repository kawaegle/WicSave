WarningsAsErrors: 'readability-identifier-naming'
HeaderFilterRegex: ''
FormatStyle: ./.clang-format
Checks: 'readability-identifier-naming,-clang-analyzer-core.UndefinedBinaryOperatorResult'
CheckOptions:
  - key:             readability-identifier-naming.ClassCase
    value:           CamelCase
  - key:             readability-identifier-naming.PrivateMemberCase
    value:           camelBack
  - key:             readability-identifier-naming.PrivateMemberPrefix
    value:           _
  - key:             readability-identifier-naming.ClassMemberPrefix
    value:           _
  - key:             readability-identifier-naming.PrivateMethodCase
    value:           CamelCase
  - key:             readability-identifier-naming.PrivateMethodPrefix
    value:           _
  - key:             readability-identifier-naming.MemberFunctionCase
    value:           CamelCase
  - key:             readability-identifier-naming.FunctionCase
    value:           CamelCase
  - key:             readability-identifier-naming.VariableCase
    value:           lower_case
  - key: readability-identifier-naming.TypeTemplateParameterIgnoredRegexp
    value:           expr-type